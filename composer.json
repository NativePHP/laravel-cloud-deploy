{
    "name": "nativephp/laravel-cloud-deploy",
    "description": "Deploy Laravel applications to Laravel Cloud from the command line",
    "keywords": ["laravel", "cloud", "deploy", "deployment"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Simon Hamp",
            "email": "simon@nativephp.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/console": "^11.0|^12.0",
        "illuminate/http": "^11.0|^12.0",
        "illuminate/support": "^11.0|^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.18",
        "orchestra/testbench": "^9.0|^10.0",
        "pestphp/pest": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "NativePhp\\LaravelCloudDeploy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NativePhp\\LaravelCloudDeploy\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NativePhp\\LaravelCloudDeploy\\CloudDeployServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
